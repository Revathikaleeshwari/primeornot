#include<stdio.h>
int main()
{
int i,num;
clrscr();
printf("Enter a number\n");
scanf("%d",&num);
if(num<=0)
{
printf("cant test for negative values\n");
goto end;
}
if(num<=3)
{
printf("given number is prime\n");
goto end;
}
for(i=2;i<=num 2;i++)
{
if(num%i==0)
{
printf("given number is not a prime  number\n");
goto end;
}
else
continue;
}
printf("given number is prime\n");
end;
getch();
}
